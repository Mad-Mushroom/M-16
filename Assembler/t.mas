;;;;;;;;;;;;;;;;;;;;;;;
; M-16 Assembler Test ;
;;;;;;;;;;;;;;;;;;;;;;;

nop ; test no operation
hlt ; halt cpu
add a, 0xff ; add 0xff to a
adr a, b ; add a and b
decrement:
dec c, 0xd5 ; subtract 0xd5 from c
der d, c ; subtract c from d
set b, 0x8a ; set b to 0x8a
jmp decrement
jmr a
jmz decrement
jmo decrement
cmp a, 0x163d
cmr a, b
je decrement
inb a
oub 0x274d
our a
sin b
sou 0x4567
sor c
mem d, 0xb75e
